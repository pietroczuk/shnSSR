{
  "name": "shineposters",
  "version": "2.0.0",
  "private": true,
  "description": "New engine with actual react hooks etc",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze": "source-map-explorer 'public_html/public/*.js'",
    "start-win": "npm run clean-win && npm-run-all --parallel dev:*",
    "clean-win": "del /s /q public_html\\public\\*client*.js public_html\\public\\*client*.js.map public_html\\server\\*server*.js*",
    "clean": "rm -if public_html/public/*client*.js public_html/public/*client*.js.map public_html/server/*.*",
    "start": "npm run clean && npm-run-all --parallel dev:*",
    "dev:start-client": "webpack  --config webpack/webpack.client.js --env env=dev --watch",
    "dev:start-server": "webpack  --config webpack/webpack.server.js --env env=dev --watch",
    "dev:server": "nodemon --watch public_html/server  --exec \"node public_html/server/server.js \" ",
    "build": "npm run clean && npm-run-all --parallel prod:*",
    "prod:build-client": "webpack --config webpack/webpack.client.js --env env=prod --watch",
    "prod:build-server": "webpack --config webpack/webpack.server.js --env env=prod --watch",
    "prod:server": "nodemon --watch public_html/server  --exec \"node public_html/server/server.js \" ",
    "build-win": "npm run clean-win && npm-run-all --parallel prod:*",
    "doc": "jsdoc -c jsdoc.json"
  },
  "author": "Andrzej Pietroczuk",
  "license": "ISC",
  "dependencies": {
    "@loadable/server": "^5.14.2",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.24.0",
    "dotenv": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.16.7",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.14.1",
    "@loadable/webpack-plugin": "^5.14.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.4",
    "css-loader": "^5.0.2",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "html-minifier": "^4.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "jsdoc": "^3.6.7",
    "node-sass": "^4.14.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.0.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.5.4",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
}
